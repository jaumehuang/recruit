/* pages/store_details/store_details.wxss */
@import "../template/foot/foot.wxss";
.container{
  margin-bottom: 120rpx;
}
page {
  background-color: #f2f2f2;
}
.user {
  background-color: #f04844;
}
.user .user-info {
  text-align: center;
  color: #fff;
  padding-bottom: 24rpx;
  position: relative;
  padding-top: 24rpx;
}

.user-info image {
  height: 160rpx;
  width: 160rpx;
  border-radius: 50%;
}
.location{
  padding:12rpx 24rpx;
  background-color: #ffffff;
}
.location .title{
  position: relative;
  padding-left:50rpx;
}
.location .title::before{
  content: "";
  display: block;
  height: 60rpx;
  width: 60rpx;
  position: absolute;
  left: -12rpx;
  top: -12rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% 100%;
}
.location .icon-weibiaoti-{
  font-size:40rpx;
}
radio-group{
 
  background-color: #ffffff;
}
label{
  padding:18rpx 24rpx;
  display: block;
  /* margin-bottom:20rpx; */
  border-bottom: 1rpx solid #f2f2f2;
}
label radio{
  margin-right:20rpx;
}
.store-list{
  margin-top:20rpx;
  background-color: #ffffff;
 
}
.store-list .store-li{
  border-bottom: 1rpx solid #f2f2f2;
   padding:16rpx 24rpx;
}