/* pages/appointment/appointment.wxss */
@import "../template/search/search.wxss";
page{
  background-color:#F2F2F2;
}
.main{
  margin-top:218rpx;
}
.main .list-li{
  padding:24rpx;
  display: flex;
  background-color: #ffffff;
  border-bottom:1rpx solid #F2F2F2;
}
.main .list-li .show-img{
  width:160rpx;
  height:160rpx;
}
.main .list-li .show-img image{
  width:100%;
  height:100%;
  border-radius: 50%;
}
.main .list-li .mian-p{
  flex: 1;
  padding-left:24rpx;
}
.main .list-li .mian-p .ellipsis-one{
  width:74%;
}
.main .list-li .mian-p .clearfix{
  margin-top:12rpx;
}