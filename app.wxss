/**app.wxss**/
@import "font/iconfont.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
}

.font-10 {
  font-size: 20rpx;
}

.font-12 {
  font-size: 24rpx;
}
.font-14 {
  font-size: 28rpx;
}
.font-16 {
  font-size: 32rpx;
}

.font-18 {
  font-size: 36rpx;
}

.c-666 {
  color: #979797;
}

.c-red {
  color: #f04844;
}

.c-333 {
  color: #333;
}

.fl {
  float: left;
}

.fl {
  float: left;
}
.fr {
  float: right;
}
.clearfix:before, .clearfix:after {
  content: "";
  display: table;
}
.clearfix:after {
  clear: both;
  overflow: hidden;
}
.clearfix {
  zoom: 1; /* IE < 8 */
}
.ellipsis-one {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.ellipsis-two {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
